{
  "name": "conectar-company-test",
  "version": "0.0.1",
  "description": "",
  "author": "Erick Marllon da Silva Carvalho",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20.18.2",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "build": "nest build",
    "prebuild": "yarn test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start dist/src/main",
    "start:prod": "nest start dist/src/main",
    "start:local": "yarn docker:up && NODE_ENV=local nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:import": "node ./scripts/migration-import.js",
    "migration:generate": "echo 'DIGITE O NOME DA MIGRATION [SEM ESPAÇO]' && read name && yarn typeorm migration:generate src/infrastructure/database/migrations/$name -d src/infrastructure/database/pgsql/data-source.ts && npm run migration:import",
    "migration:create": "echo 'DIGITE O NOME DA MIGRATION [SEM ESPAÇO]' && read name && yarn typeorm migration:create src/infrastructure/database/migrations/$name && npm run migration:import",
    "migrate:run": "yarn run typeorm -d ./src/infrastructure/database/pgsql/data-source.ts migration:run",
    "migration:revert:dev": "yarn run typeorm:dev -- -d ./src/infrastructure/database/pgsql/data-source.ts migration:revert",
    "database": "NODE_ENV=test ts-node ./node_modules/typeorm-extension/bin/cli.cjs",
    "db:create": "yarn database db:create -d src/infrastructure/database/pgsql/data-source.ts",
    "db:drop": "yarn database db:drop -d src/infrastructure/database/pgsql/data-source.ts",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/infrastructure/database/pgsql/data-source.ts",
    "seed:run": "yarn database seed:run -d src/infrastructure/database/pgsql/data-source.ts",
    "seed:create": "echo 'DIGITE O NOME DO SEED [SEM ESPAÇO]' && read name && yarn database seed:create --name=src/infrastructure/database/pgsql/seeds/$name",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check:deps": "depcheck --config .depcheckrc.js",
    "knip": "knip",
    "prepare": "husky || true",
    "auto:import": "node ./scripts/auto-import.js",
    "docker:build": "docker compose up --build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:migrate": "docker compose exec app yarn typeorm migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@aws-sdk/s3-request-presigner": "^3.876.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "argon2": "^0.43.0",
    "axios": "^1.10.0",
    "cache-manager": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "env-cmd": "^11.0.0",
    "express-session": "^1.18.1",
    "faker": "^6.6.6",
    "helmet": "^8.1.0",
    "keyv": "^5.3.4",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "nestjs-typeorm-paginate": "^4.1.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.24",
    "typeorm-extension": "^3.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.3",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "depcheck": "^1.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.61.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
