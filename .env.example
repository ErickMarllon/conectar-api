##== Environment
NODE_ENV=development
MODULES_SET=monolith

APP_NAME=conectar-api
APP_URL=http://localhost:3000
API_PREFIX=api
APP_FALLBACK_LANGUAGE=pt-br
APP_CORS_ORIGIN=http://localhost:5170,http://localhost:5173,

##== Database
DATABASE_URL=postgresql://usuario:senha@endereco:porta/nome-db?sslmode=require
DATABASE_NAME=conectar-db
DATABASE_USER=postgres
DATABASE_PASSWORD=sua_senha
DATABASE_LOGGING=false
DATABASE_MIGRATIONS_RUN=false
DATABASE_SYNCHRONIZE=true
DATABASE_MAX_CONNECTIONS=100
