JWT_FORGOT_SECRET=sua_jwt_forgot_secret
JWT_FORGOT_TOKEN_EXPIRES_IN=7D
JWT_CONFIRM_EMAIL_SECRET=sua_jwt_confirm_email_secret
JWT_CONFIRM_EMAIL_TOKEN_EXPIRES_IN=10m

##== Frontend
WEB_URL=http://localhost:5170
FRONTEND_URL=http://localhost:5170

##== Google
GOOGLE_ID=sua_google_id
